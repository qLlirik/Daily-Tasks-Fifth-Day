<Window x:Class="FifthDay.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FifthDay"
        mc:Ignorable="d"
        Title="Народная торговая компания - отед гарантийного ремонта" Height="300" Width="500" MinHeight="280" MinWidth="480" FontSize="16" Background="LightCyan" WindowStartupLocation="CenterScreen">
    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
            <Button Click="click_AcceptGoodInRepair" Content="Принять товар на гарантийный ремонт" Padding="10" Margin="10"/>
            <Button Click="click_EndRepair" Content="Завершить гарантийный ремонтй" Padding="10" Margin="10"/>
        </StackPanel>
    </Grid>
</Window>
